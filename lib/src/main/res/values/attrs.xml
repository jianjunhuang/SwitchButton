<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="switchButton">
    <attr name="invalidTextColor" format="color"/>
    <attr name="invalidBorderColor" format="color"/>
    <attr name="invalidBackgroundColor" format="color"/>

    <attr name="defaultTextColor" format="color"/>
    <attr name="defaultBorderColor" format="color"/>
    <attr name="defaultBackgroundColor" format="color"/>

    <attr name="checkedTextColor" format="color"/>
    <attr name="checkedBorderColor" format="color"/>
    <attr name="checkedBackgroundColor" format="color"/>

    <attr name="borderWidth" format="dimension|reference"/>
    <attr name="radius" format="dimension|reference"/>

    <attr name="animateDuration" format="integer"/>
  </declare-styleable>
</resources>